{
  "id": "logWeather",
  "active": true,
  "store": "weather",
  "trigger": {
    "name": "FETCH_ALL_WEATHER"
  },
  "actions": [
    {
      "type": "event",
      "event": {
        "name": "TIMER_SCHEDULE",
        "type": 2,
        "data": {
          "delay": 5000,
          "repeat": false
        }
      },
      "response": [
        {
          "type": "event",
          "event": {
            "name": "LOG",
            "data": {
              "message": "Melbourne - {{store.melbourne}}"
            }
          }
        },
        {
          "type": "event",
          "event": {
            "name": "LOG",
            "data": {
              "message": "Sydney - {{store.sydney}}"
            }
          }
        },
        {
          "type": "event",
          "event": {
            "name": "LOG",
            "data": {
              "message": "Brisbane - {{store.brisbane}}"
            }
          }
        },
        {
          "type": "event",
          "event": {
            "name": "LOG",
            "data": {
              "message": "Adelaide - {{store.adelaide}}"
            }
          }
        }
      ]
    }
  ]
}
