{
  "extends": "airbnb",
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "class-methods-use-this": "off",
    "new-cap": "off",
    "space-in-parens": ["error", "always", { "exceptions": ["{}", "()", "empty"] }],
    "no-new": "off",
    "arrow-body-style": "off",
    "max-len": "off",
  },
  "env": {
    "browser": true,
  }
}
